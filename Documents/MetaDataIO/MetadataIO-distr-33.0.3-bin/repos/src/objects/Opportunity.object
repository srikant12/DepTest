    <validationRules>
        <fullName>DEC_BADGE_EXC_RULE_6</fullName>
        <active>false</active>
        <description>Only description can be changed up to 7 days. No other field can be changed</description>
        <errorConditionFormula>AND ( 
($Profile.Name &lt;&gt; &quot;System Administrator&quot;), 
( ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Won&quot;)
   ||
   ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Lost&quot;)
),
(Owner.Id = $User.Id) ,
((TODAY() - CloseDate) &lt;= 7),
(
IF(
(ISCHANGED(Amount)|| ISCHANGED(Name) || ISCHANGED(Type)|| (Opp_Line_Item_Count__c &lt;&gt; priorvalue(Opp_Line_Item_Count__c))),true, NOT(ISCHANGED(Description))

)


)
)</errorConditionFormula>
        <errorMessage>The Opportunity record is closed. The Opportunity Owner can modify only the Opportunity description at this time. Owner cannot  change other fields in the opportunity record or add/delete Opportunity Line items.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DEC_BADGE_EXC_RULE_6a</fullName>
        <active>false</active>
        <description>Only description can be changed up to 7 days. No other field can be changed</description>
        <errorConditionFormula>AND ( 
($Profile.Name &lt;&gt; &quot;System Administrator&quot;), 
( ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Won&quot;)
   ||
   ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Lost&quot;)
),
(Owner.Id = $User.Id) ,
((TODAY() - CloseDate) &lt;= 7),
(
IF((Opp_Line_Item_Count__c &lt;&gt; priorvalue(Opp_Line_Item_Count__c)),true, NOT(ISCHANGED(Description))

)


)
)</errorConditionFormula>
        <errorMessage>The Opportunity record is closed. The Owner can modify only the Opportunity description at this time. Owner cannot add/delete Opportunity Line Items.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DEC_BADGE_EX_RULE_1</fullName>
        <active>false</active>
        <description>This record has been closed for 7 days. Only the Administrator is allowed to edit the record</description>
        <errorConditionFormula>AND ( 
($Profile.Name &lt;&gt; &quot;System Administrator&quot;),
 
  AND ( (ISPICKVAL(PRIORVALue(StageName),&quot;Closed Won&quot;)) 

  || 

  (ISPICKVAL(PRIORVALue(StageName),&quot;Closed Lost&quot;) )
 ),

 (TODAY() - CloseDate &gt; 7),

 (Owner.Id = $User.Id)




)</errorConditionFormula>
        <errorMessage>The opportunity record has been closed. Only the Administrator is allowed to modify the Opportunity record and add/delete its related records</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DEC_BADGE_EX_RULE_2</fullName>
        <active>false</active>
        <description>Dont allow status change if prior state is closed (unless it is an admin)</description>
        <errorConditionFormula>AND ( 

 ($Profile.Name &lt;&gt; &quot;System Administrator&quot;), 

 ( ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Won&quot;)
   ||
   ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Lost&quot;)
 ),
 
 (TODAY() - CloseDate &lt;= 7),

(Owner.Id &lt;&gt; $User.Id), 
  
(ISCHANGED(Description))
)</errorConditionFormula>
        <errorMessage>Only the owner or Administrator is allowed to change the status of a record after it is closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DEC_BADGE_EX_RULE_2a</fullName>
        <active>false</active>
        <description>Dont allow status change if prior state is closed (unless it is an admin)</description>
        <errorConditionFormula>AND ( 

 ($Profile.Name &lt;&gt; &quot;System Administrator&quot;), 

 ( ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Won&quot;)
   ||
   ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Lost&quot;)
 ),
(TODAY() - CloseDate &lt;= 7),
NOT(AND(
(Owner.Id = $User.Id),  
(ISCHANGED(Description))
))

)</errorConditionFormula>
        <errorMessage>The Opportunity record is closed. The Owner can modify only the Opportunity description field at this time. The Administrator has full access.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DEC_BADGE_EX_RULE_3</fullName>
        <active>false</active>
        <description>Dont allow status change if prior state is closed (unless it is an admin)</description>
        <errorConditionFormula>AND ( 

($Profile.Name &lt;&gt; &quot;System Administrator&quot;), 

( ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Won&quot;) 
|| 
ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Lost&quot;) 
), 

(Owner.Id &lt;&gt; $User.Id)
)</errorConditionFormula>
        <errorMessage>The Opportunity record is closed. Only the Owner or the Administrator is allowed to edit the opportunity record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DEC_BADGE_EX_RULE_4</fullName>
        <active>false</active>
        <description>Num of opportunity product line items cannot be changed</description>
        <errorConditionFormula>AND (
($Profile.Name &lt;&gt; &quot;System Administrator&quot;),
( ISPICKVAL(StageName,&quot;Closed Won&quot;)
   ||
  ISPICKVAL(StageName,&quot;Closed Lost&quot;)
),
(Opp_Line_Item_Count__c &lt;&gt; priorvalue(Opp_Line_Item_Count__c))

)</errorConditionFormula>
        <errorMessage>Opportunity is closed. Opportunity Products cannot be added or removed at this time</errorMessage>
