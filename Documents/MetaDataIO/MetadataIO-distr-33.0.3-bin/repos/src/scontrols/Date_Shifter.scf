		flashvars="session_id={!API.Session_ID}&server_url={!API.Partner_Server_URL_90}"	
	 	loop="false" allowScriptAccess="always" 
	 	type="application/x-shockwave-flash" 
	 	pluginspage="http://www.adobe.com/go/getflashplayer">
	</embed>
</object>
</div>
